<Window x:Class="BatsBrackenCave.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BatsBrackenCave"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Loaded="Grid_Loaded">
        <TextBlock x:Name="WelcomeTextBox" HorizontalAlignment="Left" Margin="105,66,0,0" TextWrapping="Wrap" Text="Welcome to Bats Bracken Cave" VerticalAlignment="Top" Height="38" Width="122"/>
        <Button x:Name="NextButton" Content="Next" HorizontalAlignment="Left" Margin="571,302,0,0" VerticalAlignment="Top" Width="75" Click="NextButton_Click"/>
        <TextBlock x:Name="LocationTextBox" HorizontalAlignment="Left" Margin="555,78,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="19" Width="91"><Span Foreground="Black" FontSize="12.6666666666667" FontFamily="Consolas"><Run Text="Location"/></Span></TextBlock>
        <Image x:Name="LocationImage" HorizontalAlignment="Left" Height="100" Margin="553,148,0,0" VerticalAlignment="Top" Width="100"/>
        <TextBlock x:Name="Output" HorizontalAlignment="Left" Margin="146,199,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="98" Width="195"/>

    </Grid>
</Window>
